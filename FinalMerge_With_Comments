    "# import GDP dataset and rename 'Country' column to match other datasets.\n",
    "from urllib.request import urlopen\n",
    "GDP=pd.read_pickle(urlopen(\"https://github.com/drothman9/GroupProject/raw/master/df.pkl\"),compression=None)\n",
    "GDP.columns\n",
    "OldToNew1={GDP.columns[0]:'Country'}\n",
    "GDP.rename(columns=OldToNew1,inplace=True)\n",
    "GDP.head()\n",
    "GDP"
   ]
  },
