  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "# we created dictionarys for both sides of the merge to have the flexibility to choose more favorable country names.\n",
    "# as you can see, there were some countries that had a space in the entry that needed to be corrected.\n",
    "# once corrected we made the changes using the inplace = true command.\n",
    "replacements1={'China ': 'China', \n",
    "              'United States ':'United States',\n",
    "              'European Union ':'European Union',\n",
    "              'India ':'India',\n",
    "              'Russia ':'Russia',\n",
    "              'Japan ':'Japan',\n",
    "              'Canada ':'Canada',\n",
    "              'Australia ':'Australia',\n",
    "              'United Kingdom ':\"United Kingdom\",\n",
    "              'Turkey ':'Turkey',\n",
    "              'Ivory Coast':\"Cote d'Ivoire\",\n",
    "              'Brunei':'Brunei Darussalam'\n",
    "             }\n",
    "GHG.Country.replace(replacements1,inplace=True)\n",
    "replacements2={'Iran, Islamic Rep.': 'Iran', \n",
    "              'Korea, Rep.':'South Korea',\n",
    "              'Egypt, Arab Rep.':'Egypt',\n",
    "              'Venezuela, RB':'Venezuela',\n",
    "              'Syrian Arab Republic':'Syria',\n",
    "              'Korea, Dem. People’s Rep.':'North Korea',\n",
    "              'Slovak Republic':'Slovakia',\n",
    "              'Congo, Dem. Rep.':'Democratic Republic of the Congo',\n",
    "              'Yemen, Rep.':\"Yemen\",\n",
    "              'Kyrgyz Republic':'Kyrgyzstan',\n",
    "              'Lao PDR':'Laos',\n",
    "              'Gambia, The':'Gambia',\n",
    "              'Congo, Rep.':'Republic of the Congo',              \n",
    "              'Bahamas, The':'Bahamas',\n",
    "              'St. Lucia':'Saint Lucia',\n",
    "              'Cabo Verde':\"Cape Verde\",\n",
    "              'St. Kitts and Nevis':'Saint Kitts and Nevis',\n",
    "              'St. Vincent and the Grenadines':'Saint Vincent and the Grenadines',\n",
    "              'Sao Tome and Principe':\"São Tomé and Príncipe\",\n",
    "              'Russian Federation':'Russia'\n",
    "             }\n",
    "merge0.Country.replace(replacements2,inplace=True)"
   ]
  },
